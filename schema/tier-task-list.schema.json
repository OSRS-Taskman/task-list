{
	"$schema": "https://json-schema.org/draft-07/schema",
	"title": "OSRS Taskman Mode Tier Task List",
	"type": "object",
	"required": ["name", "tasks"],
	"properties": {
		"name": {
			"type": "string",
			"maxLength": 128
		},
		"tasks": {
			"type": "array",
			"items": {
				"$ref": "./task.schema.json"
			}
		}
	}
}
